{
  "name": "avif.io",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "bundle": "ANALYZE=true npm run build",
    "convert": "cd utils && node sharp && cd ../",
    "lint": "rustywind . --write && prettier --write . && next lint --fix --dir pages --dir components",
    "develop": "npm run build && next export && cd conversion && npx cross-env NODE_ENV=production webpack && cd ../ && npm run dev",
    "release": "npm run build && next export && cd conversion && npx cross-env NODE_ENV=production webpack && cd ../ && next-sitemap --config next-sitemap.config.js && cd utils && node rss",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^12.0.4",
    "atropos": "^1.0.1",
    "firebase": "^9.4.1",
    "glob": "^7.2.0",
    "gray-matter": "^4.0.3",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "next": "^12.0.4",
    "next-images": "^1.8.1",
    "next-mdx-remote": "^3.0.8",
    "next-sitemap": "^1.6.203",
    "preact": "^10.5.15",
    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-compare-image": "^3.1.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-schemaorg": "^2.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "reading-time": "^1.5.0",
    "remark-code-titles": "^0.1.2",
    "remark-slug": "^7.0.1",
    "schema-dts": "^1.0.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/sharp": "^0.29.4",
    "autoprefixer": "^10.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-next": "^12.0.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rehype": "^12.0.0",
    "responsive-loader": "^2.3.0",
    "rss": "^1.2.2",
    "rustywind": "^0.12.2",
    "sharp": "^0.29.3",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.5.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1"
  }
}
