{
  "name": "avif.io-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-tsc": "tsc --noEmit",
    "check-eslint": "eslint src/",
    "check-style": "npx stylelint **/*.css",
    "check-ts": "npm run check-tsc && npm run check-eslint",
    "export": "npm run build && next export",
    "prettier": "prettier --write .",
    "reset": "git fetch origin && git reset --hard origin/master",
    "setup": "npm i -D && npm run export && cd conversion && npx cross-env NODE_ENV=production webpack && cd ../ && npm run dev",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.3",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@zeit/next-source-maps": "0.0.3",
    "file-saver": "^2.0.2",
    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "jimp": "^0.16.1",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.6.2",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.2.1",
    "pretty-bytes": "^5.4.1",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-compare-image": "^3.0.2",
    "react-dom": "17.0.1",
    "react-dropzone": "^11.2.4",
    "react-ios-corners": "^1.0.0",
    "react-syntax-highlighter": "^15.4.3",
    "sharp": "^0.26.3",
    "svg-sprite-loader": "^5.2.1",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.13",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-dropzone": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "responsive-loader": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.2.1"
  }
}
